  "devDependencies": {
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/node": "^22.10.2",
    "chokidar": "^4.0.1",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.7",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2"
  }
}
